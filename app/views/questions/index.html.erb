 <br>
  <h3 class="text-dark">PREGUNTAS</h3>

  <div class="text-right"><%= link_to("Preguntar", "/questions/new" , class: "btn btn-outline-success") %></div>
   <br>

  <table class="table table-sm">
    <% @questions.each do |question| %>
   
    <tr> 
    <tbody>

      <td> <div class="text-center"><span class="glyphicon glyphicon-chevron-up"></span><%= question.vote %> <br> <small class="h-75 text-muted">Vote</small><span class="glyphicon glyphicon-chevron-down"></span></div>  </td>
      <td> Respuestas </td>
      <td> <%= link_to question.title, question_path(question) %><br> <small style="color: #E82C0C; font-style: bold">Publicado por: </small></td>
      
      <% end %>
    </tbody>
    </tr>
    
  </table>


